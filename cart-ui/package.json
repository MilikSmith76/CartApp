{
  "name": "cart-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "fix": "eslint --fix",
    "prettier_base": "prettier ./{app,components,validators,services,interfaces,utils,__test__,__e2e__,__component__}/**/**.{ts,tsx,html,css}",
    "prettier": "npm run prettier_base -- --check",
    "prettify": "npm run prettier_base -- --write",
    "format": "npm run fix && npm run prettify",
    "test": "jest",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "component": "cypress open --component",
    "component:headless": "cypress run --component"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.13.2",
    "final-form": "^5.0.0",
    "lodash": "^4.17.21",
    "next": "16.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-final-form": "^7.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@headlessui/tailwindcss": "^0.2.2",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^15.9.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "eslint-plugin-perfectionist": "^5.3.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
